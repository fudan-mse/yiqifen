<!--reclaim.wxml-->
<view class="container reclaim">
  <swiper class="swiper-wapper" indicator-dots="{{indicatorDots}}"
  autoplay="{{autoplay}}" interval="{{interval}}" indicator-active-color="#fff" duration="{{duration}}">
  <block wx:for="{{imgUrls}}">
      <swiper-item>
        <image src="{{item}}" class="slide-image"/>
      </swiper-item>
    </block>
  </swiper>
  <view class="">
    <view class="">
      <img src="" alt="">
      <text class="" selectable="false" space="false" decode="false">
          电子回收
        </text>
    </view>
    <view>
        <img src="" alt="">
        <text class="" selectable="false" space="false" decode="false">
          旧衣回收
        </text>
          
    </view>
      
  </view>
    
</view>
