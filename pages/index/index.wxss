/**index.wxss**/
.top-search {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 110rpx;
  background:#123a72; 
  position: fixed;
  top: 0;
  left: 0;
}
.i-search {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  left: 40rpx;
}
.search-input {
  background: #fff;
  height: 70rpx;
  font-size: 30rpx;
  width: 93%;
  padding-left:70rpx;
  box-sizing: border-box;
}
.index-wrapper {
  height: 100%;
  padding-top: 110rpx;
  box-sizing: border-box;
  display: flex;
  width: 100%;
}
.left-cate {
  width: 300rpx;
  height: 100%;
  border-right: 1px solid #123a72;
  display: flex;
  flex-direction: column;

}
.right-content {
  height: 100%;
}
/* 左侧布局 */
.r-type {
  width: 100%;
  height: 100rpx;
  color: #123a72;
  font-size: 32rpx;
  border-bottom: 1px solid rgb(18, 58, 114);
}

.r-type.cur {
  background: #123a72;
  color: #fff;
}
.right-content .title {
  background: rgba(110, 158, 226, 0.9);
  font-size: 30rpx;
  padding: 20rpx;
  box-sizing: border-box;
  color: #000;
}
.right-content .item {
  padding: 20rpx 30rpx;
}
.right-content .item:nth-child(odd) {
  background: #f6f6f6;
}